  <%else%>
  <li><a href = "/edit_user_profile"> Edit profile</a></li>
  <li><a href = "/user_sign_out"> Sign out</a></li>
  <li><a href = "/favorites"> My favorites</a></li>
  <%end%>
  <li><a href = "/"> Home</a></li>
  </ul>
  </nav>
    <%= yield %>
  </body>

  <p style="color: green"><%= notice %></p>
  <p style="color: red"><%= alert %></p>

  
</html>
